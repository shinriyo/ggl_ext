{
    "name": "Unity Extension",
    "description": "Save Unity Documents language setting",
    "version": "0.1",
    "icons": { "48": "icon48.png",
              "128": "icon128.png" },
    "manifest_version": 2,
    "permissions": [
        "tabs", "http://docs.unity3d.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://docs.unity3d.com/*"],
            "js": ["jquery-1.8.3.min.js", "unity.js"]
        }
    ]
}
